.container-landing {
    width: 100%;
    height: 350px;
    display: grid;
    grid-template-columns: repeat(4, 2fr); /* 4 colunas */
    gap: 50px;
    padding: 20px;
    padding-right: 100px;
    justify-content: center;
    list-style-type: none;
}
.landingpage {
    width: 100%;
    height: 100%;
    padding: 20px;
    background-color:#F2C12E;
    border-radius: 15px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    color: white;
    border: 2px solid #383838;
    box-shadow: 4px 5px 0px 0px #383838;
}

.landingpage h2 {
    font-size: 1.4rem;
    color: #1A1B26;
    margin-bottom: 10px;
}
.landingpage p {
    font-size: 0.95rem;
    color: #1A1B26;
    line-height: 1.4rem;
}
